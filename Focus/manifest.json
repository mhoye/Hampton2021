{
	"manifest_version": 2,
	"name": "Focus",
	"version": "1.0.0",
	


	"background": {
		"scripts": ["/content_script.js","/popup.js"]
	},

	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"48": "icons/icon.png"
		},
		"default_title": "Focus",
		"default_popup": "/popup.html"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": []
		}
	],

	"permissions": [
		"webNavigation",
		"storage"
	]
	
}